{
    "manifest_version": 3,
    "description": "Easily snooze tabs with a click! Set timers to hide distracting tabs and automatically bring them back when you need them again.",
    "name": "Tab Snoozer - Snooze tabs in a click",
    "short_name": "Tab Snoozer",
    "author": "Rusty",
    "version": "1.0.3",
    "browser_specific_settings": {
      "gecko": {
        "id": "tabsnoozer@example.com",
        "strict_min_version": "109.0",
        "data_collection_permissions": {
          "required": ["none"]
        }
      }
    },
    "permissions": ["storage", "activeTab", "alarms", "notifications"],
    "background": {
      "scripts": ["sw.js"]
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  
